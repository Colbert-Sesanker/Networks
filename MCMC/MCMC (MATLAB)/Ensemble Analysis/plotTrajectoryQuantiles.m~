% plots and returns quantile trajectories for specified quantiles 

function quantile_trajs = plotTrajectoryQuantiles(ensemble, state)

stateName         =  ensemble.stateMap{state};
quantiles         =  ensemble.trajQuantiles{state};

 
quantile_trajs = trajectoryQuantiles(ensemble);
    
    hold on;
    figure(50);
    plot(quantile_trajs{i}); 
    xlabel('time'); 
    ylabel(stateName);
    title(['95% confidence intervals on trajectories' stateName]);
    
end % for

end 
